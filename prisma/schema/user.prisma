enum AccessTypes {
  ADMIN

  PRESIDENT
  VICEPRESIDENT

  USER

  DIRECTOR_CREATIVE
  DIRECTOR_TECHNICAL
  DIRECTOR_MARKETING
  DIRECTOR_OPERATIONS
  DIRECTOR_PR_AND_FINANCE
  DIRECTOR_HUMAN_RESOURCE

  CREATIVE
  TECHNICAL
  MARKETING
  OPERATIONS
  PR_AND_FINANCE
  HUMAN_RESOURCE
  
  ALUMNI
  EX_MEMBER
}

model user {
  id               String             @id @default(auto()) @map("_id") @db.ObjectId
  email            String             @unique
  name             String?            
  access           AccessTypes        @default(USER)
  year             String?
  img              String?
  blurhash         String?
  password         String
  rollNumber       String?
  school           String?
  college          String?
  contactNo        String?
  whatsappNo       String?  
  extra            Json?
  regForm          String[]           @default([])
  editProfileCount Int?               @default(5)
  userForms        formRegistration[]

  @@map("userdb")
}
