model user {
  id            String   @id @default(auto()) @map("_id") @db.ObjectId
  email         String   @unique
  name          String
  access        Int      @default(2)
  year          String
  password      String
  rollNumber    String
  school        String
  college       String
  contactNo     String 
  whatsappNo    String 
  member        Json?
  forms         Json?
  userForms     formRegistration[]

  @@map("userdb")
}
