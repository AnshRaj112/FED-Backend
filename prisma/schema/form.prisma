model form {
  id          String       @id @default(auto()) @map("_id") @db.ObjectId
  title       String
  description String
  date        String
  img         String?
  amount      String

  isTeam      Boolean      @default(false)
  minteamsize    Int          @default(1)
  maxteamsize    Int          @default(1)

  formFields  Json
  maxReg      Int          @default(1000)

  eventName   String
  active      Boolean      @default(true)
  
  upi         String?
  mail        String?

  userForms   formRegistration[]
}
